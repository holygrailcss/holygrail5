    "enabled": true,
    "gutter": "8px",
    "breakpoints": {
      "xs": "1px",
      "sm": "768px",
      "md": "992px",
      "lg": "1280px",
      "xl": "1440px"
    },
    "columnsXs": 12,
    "columnsXl": 24
  },
  "colors": {
    "white": "#ffffff",
    "black": "#000000",
    "dark-grey": "#737373",
    "middle-grey": "#a9a9a9",
    "light-grey": "#f0f0f0",
    "primary": "#000000",
    "error": "#b40016",
    "info": "#1a32a4",
    "success": "#76ae4a",
    "warning": "#ffc700",
    "feel": "#fb9962",
    "feel-dark": "#c94c07",
    "special": "#b99d6b",
    "bg-light": "#F0F0F0",
    "c-bg-light": "#000000",
    "sk-grey": "#e3e3e3",
    "bg-cream": "#f4f2ed"
  },
  "helpers": {
    "display": {
      "property": "display",
      "class": "d",
      "responsive": true,
      "description": "Tipo de caja de renderizado",
      "values": [
        "contents",
        "inline",
        "inline-block",
        "block",
        "flex",
        "inline-flex",
        "none"
      ]
    },
    "flex-direction": {
      "property": "flex-direction",
      "class": "flex",
      "responsive": true,
      "description": "Dirección del eje principal (horizontal o vertical)",
      "values": {
        "row": "row",
        "column": "column"
      }
    },
    "flex-wrap": {
      "property": "flex-wrap",
      "class": "flex",
      "responsive": true,
      "description": "Permite o no el ajuste en múltiples líneas",
      "values": {
        "nowrap": "nowrap",
        "wrap": "wrap"
      }
    },
    "flex-grow": {
      "property": "flex-grow",
      "class": "grow",
      "responsive": true,
      "description": "Capacidad de crecer para ocupar espacio disponible",
      "values": {
        "0": "0",
        "1": "1",
        "2": "2",
        "3": "3",
        "auto": "auto"
      }
    },
    "flex-shrink": {
      "property": "flex-shrink",
      "class": "shrink",
      "responsive": true,
      "description": "Capacidad de encogerse cuando el espacio es limitado",
      "values": {
        "0": "0",
        "1": "1",
        "2": "2",
        "3": "3",
        "auto": "auto"
      }
    },
    "order": {
      "property": "order",
      "class": "order",
      "responsive": true,
      "description": "Orden de visualización independiente del HTML",
      "values": {
        "0": "0",
        "1": "1",
        "2": "2",
        "3": "3",
        "3-neg": "-3",
        "2-neg": "-2",
        "1-neg": "-1",
        "first": "-9999",
        "last": "9999"
      }
    },
    "justify-content": {
      "property": "justify-content",
      "class": "justify",
      "responsive": true,
      "description": "Alineación en el eje principal",
      "values": {
        "stretch": "stretch",
        "start": "flex-start",
        "end": "flex-end",
        "center": "center",
        "between": "space-between",
        "around": "space-around",
        "evenly": "space-evenly"
      }
    },
    "justify-items": {
      "property": "justify-items",
      "class": "justify-items",
      "responsive": true,
      "description": "Alineación horizontal en grid",
      "values": {
        "stretch": "stretch",
        "start": "flex-start",
        "end": "flex-end",
        "center": "center",
        "between": "space-between",
        "around": "space-around",
        "evenly": "space-evenly"
      }
    },
    "align-content": {
      "property": "align-content",
      "class": "content",
      "responsive": true,
      "description": "Alineación de líneas en el eje cruzado",
      "values": {
        "stretch": "stretch",
        "start": "flex-start",
        "end": "flex-end",
        "center": "center",
        "between": "space-between",
        "around": "space-around",
        "evenly": "space-evenly"
      }
    },
    "align-items": {
      "property": "align-items",
      "class": "items",
      "responsive": true,
      "description": "Alineación en el eje cruzado",
      "values": {
        "stretch": "stretch",
        "start": "flex-start",
        "end": "flex-end",
        "center": "center",
        "baseline": "baseline"
      }
    },
    "row-gap": {
      "property": "row-gap",
      "class": "gap-y",
      "responsive": true,
      "description": "Espacio entre filas",
      "useSpacing": true
    },
    "column-gap": {
      "property": "column-gap",
      "class": "gap-x",
      "responsive": true,
      "description": "Espacio entre columnas",
      "useSpacing": true
    },
    "gap": {
      "property": "gap",
      "class": "gap",
      "responsive": true,
      "description": "Espacio entre filas y columnas",
      "useSpacing": true
    },
    "text-align": {
      "property": "text-align",
      "class": "text",
      "responsive": true,
      "description": "Alineación del texto",
      "values": {
        "left": "left",
        "center": "center",
        "right": "right",
        "justify": "justify"
      }
    }
  },
